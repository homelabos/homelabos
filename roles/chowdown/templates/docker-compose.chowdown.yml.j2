---
version: '3'

networks:
  traefik_network:
    external:
      name: homelabos_traefik

services:
  app:
    image: gregyankovoy/chowdown:{{ chowdown.version }}
    restart: unless-stopped
    networks:
      - traefik_network
    volumes:
      - "{{ volumes_root }}/chowdown/config:/config"
{% include './labels.j2' %}
